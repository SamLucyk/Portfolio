    <script>
        $('.project-item').each(function(){
            
            $(this).find( '.project-title-container' ).prepend("<div class=\"project-button\"><span class=\"showproject\">See More</span><span class=\"hideproject\">See Less</span></div>")
            
            var detail = $(this).find( ".project-detail" );
            var hide = $(this).find( ".hideproject" );
            var show = $(this).find( ".showproject" );
            var parent = $(this);
            
            show.click(function(){
                detail.show();
                $(this).hide();
                hide.show();
                $('html, body').animate({
                    scrollTop: parent.offset().top - 5
                }, 1000);
            });
            
            hide.click(function(){
                detail.hide();
                $(this).hide();
                show.show();
            });
        })
    </script>
